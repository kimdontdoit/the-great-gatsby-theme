{
  "name": "@kimdontdoit/the-great-gatsby-theme",
  "description": "A great base for kimdontdoit gatsby-based projects",
  "version": "1.1.2",
  "main": "gatsby-config.js",
  "author": "kimdontdoit",
  "scripts": {
    "develop": "npm run clean && gatsby develop -o -S",
